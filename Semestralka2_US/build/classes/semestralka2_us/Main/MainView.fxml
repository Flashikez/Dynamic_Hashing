<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" HBox.hgrow="ALWAYS">
         <cursor>
           
         </cursor>
         <children>
            <ToolBar>
               <items>
                  <MenuButton fx:id="menuZadania" mnemonicParsing="false" text="Zadania">
                     <items>
                        <MenuItem fx:id="mi1" mnemonicParsing="false" text="Zadanie č.1" />
                        <MenuItem fx:id="mi2" mnemonicParsing="false" text="Zadanie č.2" />
                        <MenuItem fx:id="mi3" mnemonicParsing="false" text="Zadanie č.3" />
                        <MenuItem fx:id="mi4" mnemonicParsing="false" text="Zadanie č.4" />
                        <MenuItem fx:id="mi5" mnemonicParsing="false" text="Zadanie č.5" />
                     </items></MenuButton>
                  <Pane maxWidth="1.7976931348623157E308" minWidth="-Infinity" />
                  <Button fx:id="btnUloz" mnemonicParsing="false" text="Uloz do suborov" />
                  <MenuButton fx:id="mbFiles" mnemonicParsing="false" text="Zobrazenie súborov" textFill="#0926e1">
                    <items>
                        <Menu mnemonicParsing="false" text="Súbor ID ">
                          <items>
                            <MenuItem fx:id="miIDFULL" mnemonicParsing="false" text="Dátový súbor" />
                              <MenuItem fx:id="miIDOVER" mnemonicParsing="false" text="Preplňujúci súbor" />
                            <MenuItem fx:id="miIDCON" mnemonicParsing="false" text="Prepojenie blokov" />
                          </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Súbor súpisné číslo, kataster">
                          <items>
                              <MenuItem fx:id="miSCFULL" mnemonicParsing="false" text="Dátový súbor" />
                            <MenuItem fx:id="miSCOVER" mnemonicParsing="false" text="Preplňujúci súbor" />
                              <MenuItem fx:id="miSCCON" mnemonicParsing="false" text="Prepojenie blokov" />
                          </items>
                        </Menu>
                      <MenuItem fx:id="miData" mnemonicParsing="false" text="Neusporiadaný súbor" />
                    </items>
                  </MenuButton>
                  <Text fx:id="txtFileSize" strokeType="OUTSIDE" strokeWidth="0.0">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Text>
               </items>
            </ToolBar>
            <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true" style="-fx-background-color: &quot;transparent&quot;;" VBox.vgrow="ALWAYS">
               <content>
                  <TextFlow fx:id="tfOutput" lineSpacing="5.0" textAlignment="CENTER" />
               </content></ScrollPane>
         </children>
      </VBox>
   </children>
</HBox>
